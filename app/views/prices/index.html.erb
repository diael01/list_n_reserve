<h1>Listing prices</h1>
<!--<%=params[:facilityId].inspect%>-->
<% if !@prices.nil? %>
      <table>
        <thead>
          <tr>
            <th>Reservation Type</th>
            <th>Reservation Price</th>
            <th>Pricing Description</th>
            <th>Facility Id</th>
            <th>Players</th>
            <th></th>
            <th></th>
            <th></th>
          </tr>
        </thead>

        <tbody>
          <% @prices.each do |price| %>
            <tr>
              <td><%= price.reservationType %></td>
              <td><%= price.reservationPrice %></td>
              <td><%= price.pricingDescription %></td>
              <td><%= price.facilityId %></td>
              <td><%= price.players %></td>
              <td><%= link_to 'Show', price %></td>
              <td><%= link_to 'Edit', edit_price_path(price) %></td>
              <td><%= link_to 'Destroy', price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
<% end %>
<br>

<%= link_to 'New Price', new_price_path(:facilityId =>params[:facilityId]) %>
